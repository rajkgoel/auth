{
  "name": "auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "tsc && node build/index.js",
    "build": "rmdir /s /q dist && webpack --mode development && xcopy /e .\\node_modules .\\dist\\node_modules\\",
    "build-prod": "rmdir /s /q dist && webpack --mode production && xcopy /e .\\node_modules .\\dist\\node_modules\\",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\""
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
